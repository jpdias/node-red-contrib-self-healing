{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "mutate": [
    "action-delay/*.js",
    "balancing/*.js",
    "compensate/*.js",
    "flow-control/*.js",
    "heartbeat/*.js",
    "kalman-noise-filter/*.js",
    "network-aware/*.js",
    "readings-watcher/*.js",
    "redundancy/*.js",
    "replication-voter/*.js",
    "threshold-check/*.js",
    "timing-check/*.js"
  ],
  "reporters": ["html", "clear-text", "progress"],
  "testRunner": "mocha",
  "coverageAnalysis": "perTest"
}
